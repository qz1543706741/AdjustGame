<view class="select-box">
  <view class="select-current" catchtap="openClose">
    <text class="current-name">{{current.value}}</text>
  </view>
  <view class="option-list" wx:if="{{isShow}}" catchtap="optionTap">
    <text class="option" wx:for="{{options}}" wx:key="index" data-key="{{item[optionsKeys[0]]}}"
      data-value="{{item[optionsKeys[1]]}}">({{item[optionsKeys[0]]}}) {{item[optionsKeys[1]]}}
    </text>
  </view>
</view>